using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace Mars.Classes
{

    /// <summary>
    /// An individual room inside the lobby.
    /// </summary>
    public class Room
    {

        /// <summary>
        /// The name of the room.
        /// </summary>
        public string Name { get; set; }

        /// <summary>
        /// A list of users in the room, should never be more than two.
        /// </summary>
        public List<UserInformation> Users { get; set; } 

    }

}
